# Alchemy API 密钥 - 用于访问以太坊节点
ALCHEMY_API_KEY=your_alchemy_api_key_here

# 部署钱包私钥 - 用于部署合约（请保持私密！）
__RUNTIME_DEPLOYER_PRIVATE_KEY=your_private_key_here

# 区块浏览器 API 密钥 - 用于验证合约
ETHERSCAN_MAINNET_API_KEY=your_etherscan_api_key_here
ETHERSCAN_OPTIMISTIC_API_KEY=your_optimistic_etherscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# 其他可能需要的环境变量
# REPORT_GAS=true  # 启用 gas 报告
# COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here  # 用于 gas 报告中的美元转换

# 主网分叉设置
MAINNET_FORKING_ENABLED=false  # 设置为 true 以启用主网分叉
